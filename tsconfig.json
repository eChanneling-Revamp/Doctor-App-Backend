{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "src",
    "paths": {
      "@services/*": ["services/*"],
      "@controllers/*": ["controllers/*"],
      "@utils/*": ["utils/*"]
    }


  }
}
